<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="630.0" prefWidth="1300.0" stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="poker.app.view.RootLayoutController">
   <top>
      <MenuBar fx:id="mb" prefHeight="35.0" prefWidth="150.0">
        <menus>
          <Menu fx:id="m" mnemonicParsing="false" text="Game">
            <items>
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
          <Menu fx:id="about" mnemonicParsing="false" onAction="#handleAbout" text="About">
          </Menu>
          <Menu fx:id="close" mnemonicParsing="false" onAction="#handleExit" text="Close">
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
